fun int(int) isPrime(n) =
    var int i = 2;
    while i < n do
        if n % i == 0 then
            return 0;
        end
        i = i + 1;
    end
    return 1;
end


fun int() main() =
    var int n = 2;
    while 1 do
        if isPrime(n) then
            print(n);
        end
        n = n + 1;
    end
end
